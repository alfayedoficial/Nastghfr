<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundimg_main"
    tools:context=".view.fragment.Taspi7AfterSalahFragment">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout_Taspi7AfterSalah"
        android:layout_width="270dp"
        android:layout_height="60dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/background_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/txt_Taspi7AfterSalah"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="8dp"
            android:fontFamily="@font/ayman15"
            android:gravity="center"
            android:text="التسبيح بعد كل صلاة"
            android:textColor="@color/background_nav"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/txtText_Taspi7AfterSalah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:fontFamily="@font/bein_normal"
        android:gravity="center"
        android:singleLine="false"
        android:textColor="@color/headerColor"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout_Taspi7AfterSalah"
        tools:text="سبحان الله" />

    <TextView
        android:id="@+id/txtBasicCounter_Taspi7AfterSalah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/bein_normal"
        android:gravity="center"
        android:textColor="@color/headerColor"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/txtText_Taspi7AfterSalah"
        app:layout_constraintStart_toStartOf="@+id/txtText_Taspi7AfterSalah"
        app:layout_constraintTop_toBottomOf="@+id/txtText_Taspi7AfterSalah"
        tools:text="0" />

    <TextView
        android:id="@+id/txtBasicCounter_Taspi7AfterSalah2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:fontFamily="@font/bein_normal"
        android:gravity="center"
        android:text="( "
        android:textColor="@color/headerColor"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/txtBasicCounter_Taspi7AfterSalah"
        app:layout_constraintEnd_toStartOf="@+id/txtBasicCounter_Taspi7AfterSalah"
        app:layout_constraintTop_toTopOf="@+id/txtBasicCounter_Taspi7AfterSalah" />

    <TextView
        android:id="@+id/txtBasicCounter_Taspi7AfterSalah3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="5dp"
        android:fontFamily="@font/bein_normal"
        android:gravity="center"
        android:text=")"
        android:textColor="@color/headerColor"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/txtBasicCounter_Taspi7AfterSalah"
        app:layout_constraintStart_toEndOf="@+id/txtBasicCounter_Taspi7AfterSalah"
        app:layout_constraintTop_toTopOf="@+id/txtBasicCounter_Taspi7AfterSalah" />

    <TextView
        android:id="@+id/txtCounter_Taspi7AfterSalah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/bein_normal"
        android:gravity="center"
        android:text="0"
        android:textColor="@color/headerColor"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/txtBasicCounter_Taspi7AfterSalah"
        app:layout_constraintStart_toStartOf="@+id/txtBasicCounter_Taspi7AfterSalah"
        app:layout_constraintTop_toBottomOf="@+id/txtBasicCounter_Taspi7AfterSalah" />

    <ImageButton
        android:id="@+id/imgBtnOnClick_Taspi7AfterSalah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@null"
        android:src="@drawable/ic_fingerprint"
        app:layout_constraintEnd_toEndOf="@+id/txtCounter_Taspi7AfterSalah"
        app:layout_constraintStart_toStartOf="@+id/txtCounter_Taspi7AfterSalah"
        app:layout_constraintTop_toBottomOf="@+id/txtCounter_Taspi7AfterSalah" />

</androidx.constraintlayout.widget.ConstraintLayout>